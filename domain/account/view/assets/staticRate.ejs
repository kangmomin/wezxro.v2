<div class="modal-dialog modal-lg">
    <div class="modal-content">
        <div class="modal-header">
            <h4 class="modal-title"></i> Edit Custom rate (<%= u.email %>)</h4>
            <button type="button" class="close" data-dismiss="modal"></button>
        </div>
        <form action="/admin/users/staticRate/" class="form actionForm" method="POST" accept-charset="utf-8" data-redirect="/admin/users">
            <input type="hidden" name="ids" value="<%= u.userId %>" />
            <div class="modal-body">
                <div class="o-auto" style="height: 10rem; overflow: auto;">
                    <ul class="list-unstyled list-separated services-group-items">
                        <div class="s-items">
                            <input type="number" class="form-control" name="staticRate" value="<%= u.customRate %>">
                        </div>
                    </ul>
                </div>

            </div>
            <div class="card-footer text-right">
                <div class="d-flex">
                    <button type="submit" class="btn btn-primary ml-auto mr-2">Save</button>
                    <button type="button" class="btn btn-dark" data-dismiss="modal">Close</button>
                </div>
            </div>
        </form>
    </div>
</div>